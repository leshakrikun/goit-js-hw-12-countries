{{#this }}
<div class="country-card"></div>
<h2 class="country-name">{{name}}</h2>
<div class="info-card">
    <div class="info">
    <p class="capital"><span>Capital: </span>{{capital}}</p>
    <p class="population"><span>Population: </span>{{population}}</p>
    <ul class="languages-list"><span>languages</span>
            {{#each languages}}
                <li class="languages-list__item">{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <img src={{flag}} alt="flag" class="flag">
</div>
{{/this}} 
